{
  "publisher": "howcasperwhat",
  "name": "mathjax-webview",
  "displayName": "MathJax Webview",
  "version": "0.0.1",
  "private": true,
  "description": "A VSCode extensions to preview and edit MathJax in webview.",
  "author": "Casper Huang",
  "repository": {
    "type": "git",
    "url": "https://github.com/howcasperwhat/mathjax-webview.git"
  },
  "categories": [
    "Visualization"
  ],
  "main": "dist/extension/index.js",
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.84.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "mathjax-webview.show",
        "title": "Webview Vue: Show"
      }
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "update": "vscode-ext-gen --output extension/generated/meta.ts",
    "vscode:prepublish": "npm run build",
    "pack": "vsce package",
    "publish": "vsce publish",
    "prepare": "npm run update"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@tomjs/vite-plugin-vscode": "^4.0.0",
    "@types/node": "^22.14.1",
    "@types/vscode": "^1.75.0",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vscode/vsce": "^3.3.2",
    "eslint": "^9.24.0",
    "jiti": "^2.4.2",
    "reactive-vscode": "^0.2.14",
    "vite": "^6.2.6",
    "vscode-ext-gen": "^1.0.2",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  }
}
